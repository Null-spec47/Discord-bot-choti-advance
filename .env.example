# Discord Bot Tokens Configuration Template
# Copy this file to .env and replace the placeholder values with actual tokens
# Get your tokens from Discord Developer Portal: https://discord.com/developers/applications

# Main Bot Tokens (Required) - Replace with actual Discord bot tokens
BOT1_TOKEN=
BOT2_TOKEN=
BOT3_TOKEN=
BOT4_TOKEN=
BOT5_TOKEN=
BOT6_TOKEN=

# Additional Bot Token (Optional)
BOT7_TOKEN=

# Security Configuration (Optional - Default values will be used if not set)
ENCRYPTION_KEY=
MAX_RETRIES=
RETRY_DELAY=
TOKEN_ROTATION_INTERVAL=

# Anti-Detection Settings (Optional - Set to 'true' to enable)
USER_AGENT_ROTATION=
MESSAGE_PATTERN_ROTATION=
ACTIVITY_ROTATION=
STATUS_ROTATION=
TYPING_SIMULATION=
REACTION_SIMULATION=

# Rate Limiting (Optional - Default values allow unlimited)
MAX_MESSAGES_PER_MINUTE=
MAX_COMMANDS_PER_MINUTE=
MAX_REACTIONS_PER_MINUTE=
MAX_JOINS_PER_HOUR=
MAX_LEAVES_PER_HOUR=

# Stealth Mode (Optional - Default is enabled)
STEALTH_MODE=
HUMAN_BEHAVIOR=
ANTI_DETECTION=

# Discord API Configuration (Optional)
API_VERSION=
GATEWAY_VERSION=
INTENTS=

# Web Panel Configuration (Optional)
PORT=
ADMIN_PASSWORD=